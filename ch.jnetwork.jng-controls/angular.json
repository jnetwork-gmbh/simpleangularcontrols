        "ct": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "jngcontrols-bootstrap3:serve",
            "watch": true,
            "headless": false,
            "testingType": "component"
          },
          "configurations": {
            "development": {
              "devServerTarget": "jngcontrols-bootstrap3:serve:development"
