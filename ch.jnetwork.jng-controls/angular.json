{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "module-base": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": ["src/assets", "src/favicon.ico"],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "module-base:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "module-base:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "module-base:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [],
            "assets": ["src/assets", "src/favicon.ico"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "module-base-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "module-base:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "jngcontrols-common": {
      "root": "projects/jngcontrols-common",
      "sourceRoot": "projects/jngcontrols-common/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/jngcontrols-common/tsconfig.lib.json",
            "project": "projects/jngcontrols-common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/jngcontrols-common/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jngcontrols-common/src/test.ts",
            "tsConfig": "projects/jngcontrols-common/tsconfig.spec.json",
            "karmaConfig": "projects/jngcontrols-common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jngcontrols-common/tsconfig.lib.json",
              "projects/jngcontrols-common/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "**/treeview/**", // Temp Rule
              "**/tinymce/**" // Temp Rule
            ]
          }
        }
      }
    },
    "jngcontrols-bootstrap4": {
      "root": "projects/jngcontrols-bootstrap4",
      "sourceRoot": "projects/jngcontrols-bootstrap4/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/jngcontrols-bootstrap4/tsconfig.lib.json",
            "project": "projects/jngcontrols-bootstrap4/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/jngcontrols-bootstrap4/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jngcontrols-bootstrap4/src/test.ts",
            "tsConfig": "projects/jngcontrols-bootstrap4/tsconfig.spec.json",
            "karmaConfig": "projects/jngcontrols-bootstrap4/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jngcontrols-bootstrap4/tsconfig.lib.json",
              "projects/jngcontrols-bootstrap4/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "jngcontrols-bootstrap3": {
      "root": "projects/jngcontrols-bootstrap3",
      "sourceRoot": "projects/jngcontrols-bootstrap3/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/jngcontrols-bootstrap3/tsconfig.lib.json",
            "project": "projects/jngcontrols-bootstrap3/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/jngcontrols-bootstrap3/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jngcontrols-bootstrap3/src/test.ts",
            "tsConfig": "projects/jngcontrols-bootstrap3/tsconfig.spec.json",
            "karmaConfig": "projects/jngcontrols-bootstrap3/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jngcontrols-bootstrap3/tsconfig.lib.json",
              "projects/jngcontrols-bootstrap3/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "demo-bootstrap4": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/demo-bootstrap4",
      "sourceRoot": "projects/demo-bootstrap4/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-bootstrap4",
            "index": "projects/demo-bootstrap4/src/index.html",
            "main": "projects/demo-bootstrap4/src/main.ts",
            "polyfills": "projects/demo-bootstrap4/src/polyfills.ts",
            "tsConfig": "projects/demo-bootstrap4/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/demo-bootstrap4/src/favicon.ico",
              "projects/demo-bootstrap4/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              }
            ],
            "styles": ["projects/demo-bootstrap4/src/styles.scss"],
            "scripts": [
              "./node_modules/quill/dist/quill.min.js",
              "./node_modules/tinymce/tinymce.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-bootstrap4/src/environments/environment.ts",
                  "with": "projects/demo-bootstrap4/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-bootstrap4:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-bootstrap4:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-bootstrap4:build"
          }
        }
      }
    }
  },
  "defaultProject": "module-base",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
