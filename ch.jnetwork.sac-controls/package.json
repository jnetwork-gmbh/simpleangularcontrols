{
  "name": "sac-controls",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "demo-bs3": "ng serve demo-bootstrap3 --port 4200",
    "demo-bs4": "ng serve demo-bootstrap4 --port 4201",
    "demo-bs5": "ng serve demo-bootstrap5 --port 4202",
    "build-common": "ng build --project sac-common",
    "build-bs3": "ng build --project sac-bootstrap3",
    "build-bs4": "ng build --project sac-bootstrap4",
    "build-bs5": "ng build --project sac-bootstrap5",
    "run-common": "ng build --project sac-common --watch",
    "run-bs3": "ng build --project sac-bootstrap3 --watch",
    "run-bs4": "ng build --project sac-bootstrap4 --watch",
    "run-bs5": "ng build --project sac-bootstrap5 --watch",
    "lint-common": "ng lint sac-common",
    "lint-bs3": "ng lint sac-bootstrap3",
    "lint-bs4": "ng lint sac-bootstrap4",
    "lint-bs5": "ng lint sac-bootstrap5",
    "compodoc-bs3": "npx compodoc -c bootstrap3.compodocrc.json",
    "compodoc-bs4": "npx compodoc -c bootstrap4.compodocrc.json",
    "compodoc-bs5": "npx compodoc -c bootstrap5.compodocrc.json",
    "doc-bs3": "start http://localhost:8888 & npx compodoc -r 8888 -s -d ../docs/bootstrap3",
    "doc-bs4": "start http://localhost:8888 & npx compodoc -r 8888 -s -d ../docs/bootstrap4",
    "doc-bs5": "start http://localhost:8888 & npx compodoc -r 8888 -s -d ../docs/bootstrap5",
    "test-edit-bs3": "npx cypress open -C projects/sac-bootstrap3/cypress.config.ts --env coverage=true",
    "test-edit-bs4": "npx cypress open -C projects/sac-bootstrap4/cypress.config.ts --env coverage=true",
    "test-edit-bs5": "npx cypress open -C projects/sac-bootstrap5/cypress.config.ts --env coverage=true",
    "test-bs3": "npm run build-common && npm run instrument-common && npx cypress run --component -C projects/sac-bootstrap3/cypress.config.ts --env coverage=true",
    "test-bs4": "npm run build-common && npm run instrument-common && npx cypress run --component -C projects/sac-bootstrap4/cypress.config.ts --env coverage=true",
    "test-bs5": "npm run build-common && npm run instrument-common && npx cypress run --component -C projects/sac-bootstrap5/cypress.config.ts --env coverage=true",
    "ngcc": "ngcc",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "instrument-common": "npx nyc instrument ./dist/sac-common --in-place"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.5",
    "@angular/common": "10.1.5",
    "@angular/compiler": "10.1.5",
    "@angular/core": "10.1.5",
    "@angular/forms": "10.1.5",
    "@angular/localize": "10.1.5",
    "@angular/platform-browser": "10.1.5",
    "@angular/platform-browser-dynamic": "10.1.5",
    "@angular/router": "10.1.5",
    "@tinymce/tinymce-angular": "4.2.4",
    "angular-imask": "6.0.5",
    "core-js": "^3.6.5",
    "moment": "2.29.1",
    "ngx-toastr": "13.1.0",
    "ngx-uploadx": "3.5.2",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.5.5",
    "tinymce": "^5.8.1",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.6",
    "@angular-devkit/build-ng-packagr": "0.1001.6",
    "@angular/cli": "10.1.6",
    "@angular/compiler-cli": "10.1.5",
    "@angular/language-service": "10.1.5",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "^2.0.6",
    "@types/jquery": "3.3.38",
    "@types/node": "^12.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "3.3.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.0",
    "typescript": "3.9.7"
  }
}