{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "es2020",
    "baseUrl": "./",
    "paths": {
      "@jnetwork/jngcontrols-common": [
        "dist/jngcontrols-common"
      ],
      "@jnetwork/jngcontrols-common/*": [
        "dist/jngcontrols-common/*"
      ],
      "@jnetwork/jngcontrols-bootstrap4": [
        "dist/jngcontrols-bootstrap4"
      ],
      "@jnetwork/jngcontrols-bootstrap4/*": [
        "dist/jngcontrols-bootstrap4/*"
      ],
      "@jnetwork/jngcontrols-bootstrap3": [
        "dist/jngcontrols-bootstrap3"
      ],
      "@jnetwork/jngcontrols-bootstrap3/*": [
        "dist/jngcontrols-bootstrap3/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false,
    "allowEmptyCodegenFiles": true
  }
}
